.wrapper{
  width: "100%";
}
.banner{
  width: 100%;
  height: 300rpx;
  position: relative;
}
.banner-title{
  font-size:80rpx;
  font-family:PingFangSC-Regular;
  font-weight:bold;
  color:rgba(235,223,158,1);
  position: absolute;top: 70rpx;left: 28rpx;
}
.banner-time{
  font-size: 28rpx;
  font-family:PingFangSC-Regular;
  font-weight:bold;
  color:rgba(235,223,158,1);
  position: absolute;top: 170rpx;left: 28rpx;
}
.banner-pic{
  width: 100%;
  height: 300rpx;
}

.list-container{
  width: 100%;
  min-height: 200rpx;
  background:rgba(255,255,255,1);
  border-radius:52rpx 52rpx 0 0;
  position: relative;top: -45rpx;
  padding-top: 55rpx;
}

.item-list{
  width: 100%;
  min-height: 200rpx;
}
.list{
  padding: 10rpx 10rpx 10rpx 10rpx;
  display: flex;
  align-items: center;
  position: relative;
}
.item-img{
  display: inline-block;
  width: 56rpx;
  height: 48rpx;
  margin-left: 40rpx;
  text-align: center;
  line-height: 48rpx;
}
.item-name{
  font-size:34rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  margin-left: 80rpx;
}
.item-core{
  font-size:34rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(200,160,21,1);
  position: absolute;right: 85rpx;
}
.class0{
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: 100% 100%;
  color: white;
}
.class1{
  color: white;
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: 100% 100%;
}
.class2{
  color: white;
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: 100% 100%;
}